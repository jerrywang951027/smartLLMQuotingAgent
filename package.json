{
  "name": "smart-llm-quoting-agent",
  "version": "1.0.0",
  "description": "A full-stack AI agent application that integrates multiple Large Language Models (LLMs) with Model Context Protocol (MCP) servers for intelligent task execution and tool usage.",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd server && npm start",
    "client": "cd client && npm start",
    "install-all": "npm install && cd server && npm install && cd ../client && npm install",
    "build": "cd client && npm run build",
    "start": "cd server && npm start",
    "test": "cd server && npm test && cd ../client && npm test"
  },
  "keywords": [
    "ai",
    "llm",
    "agent",
    "langchain",
    "react",
    "nodejs",
    "mcp",
    "model-context-protocol",
    "gemini",
    "openai",
    "claude",
    "weather",
    "tools"
  ],
  "author": "Jin Wang",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jerrywang951027/smartLLMQuotingAgent.git"
  },
  "bugs": {
    "url": "https://github.com/jerrywang951027/smartLLMQuotingAgent/issues"
  },
  "homepage": "https://github.com/jerrywang951027/smartLLMQuotingAgent#readme",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}